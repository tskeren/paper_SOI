file = {:C$\backslash$:/praca/box/Box Sync/Tomas/structured/literature/papers/zyvex/Longo - 2016 - Toward atomic-scale patterned atomic layer deposition{\_} reactions of al2o3 precursor on a Si(001) surface with mixed functionalizations.pdf:pdf},
isbn = {1932-7447},
