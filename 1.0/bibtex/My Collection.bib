issn = {13672630},
journal = {New Journal of Physics},
keywords = {Coulomb blockade,Dopant devices,HRL,IBM,Quantum dots,Quantum transport,Scaning tunneling microscopy,Transport spectroscopy},
mendeley-tags = {HRL,IBM},
number = {8},
publisher = {IOP Publishing},
title = {{Tunnel barrier design in donor nanostructures defined by hydrogen-resist lithography}},
