issn = {1748-3387},
journal = {Nature Nanotechnology},
number = {January},
pages = {1--26},
pmid = {26727201},
title = {{Single-Spin CCD}},
url = {http://www.nature.com/doifinder/10.1038/nnano.2015.291{\%}0Ahttp://arxiv.org/abs/1507.07991},
volume = {11},
year = {2015}
}
