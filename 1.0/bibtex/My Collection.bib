@article{Lai2011,
abstract = {Double quantum dots are convenient solid-state platforms to encode quantum information. Two-electron spin states can be detected and manipulated using quantum selection rules based on the Pauli exclusion principle, leading to Pauli spin blockade of electron transport for triplet states. Coherent spin states would be optimally preserved in an environment free of nuclear spins, which is achievable in silicon by isotopic purification. Here we report on a deliberately engineered, gate-defined silicon metal-oxide-semiconductor double quantum dot system. The electron occupancy of each dot and the inter-dot tunnel coupling are independently tunable by electrostatic gates. At weak inter-dot coupling we clearly observe Pauli spin blockade and measure a large intra-dot singlet-triplet splitting {\textgreater} 1 meV. The leakage current in spin blockade has a peculiar magnetic field dependence, unrelated to electron-nuclear effects and consistent with the effect of spin-flip cotunneling processes. The results obtained here provide excellent prospects for realising singlet-triplet qubits.},
archivePrefix = {arXiv},
arxivId = {1012.1410},
author = {Lai, N S and Lim, W H and Yang, C H and Zwanenburg, F a and Coish, W a and Qassemi, F and Morello, a and Dzurak, a S},
doi = {10.1038/srep00110},
eprint = {1012.1410},
file = {:C$\backslash$:/praca/box/Box Sync/Tomas/structured/literature/papers/Lai - 2011 - Pauli Spin Blockade in a Highly Tunable Silicon Double Quantum Dot.pdf:pdf},
issn = {2045-2322},
journal = {Scientific reports},
keywords = {Electric Capacitance,Electron Transport,Equipment Design,Microscopy, Electron, Scanning,Nanotechnology,Quantum Dots,Quantum Theory,Semiconductors,Silicon},
pages = {110},
pmid = {22355627},
title = {{Pauli spin blockade in a highly tunable silicon double quantum dot.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3216592{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {1},
year = {2011}
}
