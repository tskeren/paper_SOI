doi = {10.1038/srep06219},
file = {:C$\backslash$:/praca/box/Box Sync/Tomas/structured/literature/papers/Moraru - 2014 - Transport spectroscopy of coupled donors in silicon nano-transistors.pdf:pdf},
issn = {2045-2322},
