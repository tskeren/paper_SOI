title = {{Tunnel coupled dangling bond structures on hydrogen terminated silicon surfaces}},
volume = {134},
year = {2011}
}
@article{Huber2010,
abstract = {A scanning microwave microscope (SMM) for spatially resolved capacitance measurements in the attofarad-to-femtofarad regime is presented. The system is based on the combination of an atomic force microscope (AFM) and a performance network analyzer (PNA). For the determination of absolute capacitance values from PNA reflection amplitudes, a calibration sample of conductive gold pads of various sizes on a SiO(2) staircase structure was used. The thickness of the dielectric SiO(2) staircase ranged from 10 to 200 nm. The quantitative capacitance values determined from the PNA reflection amplitude were compared to control measurements using an external capacitance bridge. Depending on the area of the gold top electrode and the SiO(2) step height, the corresponding capacitance values, as measured with the SMM, ranged from 0.1 to 22 fF at a noise level of {\~{}}2 aF and a relative accuracy of 20{\%}. The sample capacitance could be modeled to a good degree as idealized parallel plates with the SiO(2) dielectric sandwiched in between. The cantilever/sample stray capacitance was measured by lifting the tip away from the surface. By bringing the AFM tip into direct contact with the SiO(2) staircase structure, the electrical footprint of the tip was determined, resulting in an effective tip radius of {\~{}}60 nm and a tip-sample capacitance of {\~{}}20 aF at the smallest dielectric thickness.},
author = {Huber, H. P. and Moertelmaier, M. and Wallis, T. M. and Chiang, C. J. and Hochleitner, M. and Imtiaz, A. and Oh, Y. J. and Schilcher, K. and Dieudonne, M. and Smoliner, J. and Hinterdorfer, P. and Rosner, S. J. and Tanbakuchi, H. and Kabos, P. and Kienberger, F.},
doi = {10.1063/1.3491926},
file = {:C$\backslash$:/praca/box/Box Sync/Tomas/structured/literature/papers/scanning{\_}microwave/2010-RevSciInstrum-Huber{\_}Ferry-ScanningMicrowaveMicroscope.pdf:pdf},
isbn = {0034-6748},
issn = {00346748},
journal = {Review of Scientific Instruments},
number = {11},
pmid = {21133472},
title = {{Calibrated nanoscale capacitance measurements using a scanning microwave microscope}},
volume = {81},
year = {2010}
}
