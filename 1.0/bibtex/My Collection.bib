journal = {Journal of physics. Condensed matter : an Institute of Physics journal},
keywords = {Arsenic,Arsenic: chemistry,Cold Temperature,Crystallography,Microscopy, Scanning Tunneling,Silicon,Silicon: chemistry,Surface Properties},
number = {1},
pages = {012001},
pmid = {24304933},
