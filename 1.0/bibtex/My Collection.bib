@article{House2015,
abstract = {Spin states of the electrons and nuclei of phosphorus donors in silicon are strong candidates for quantum information processing applications given their excellent coherence times. Designing a scalable donor-based quantum computer will require both knowledge of the relationship between device geometry and electron tunnel couplings, and a spin readout strategy that uses minimal physical space in the device. Here we use radio frequency reflectometry to measure singlet-triplet states of a few-donor Si:P double quantum dot and demonstrate that the exchange energy can be tuned by at least two orders of magnitude, from 20 $\mu$eV to 8 meV. We measure dot-lead tunnel rates by analysis of the reflected signal and show that they change from 100 MHz to 22 GHz as the number of electrons on a quantum dot is increased from 1 to 4. These techniques present an approach for characterizing, operating and engineering scalable qubit devices based on donors in silicon.},
author = {House, M G and Kobayashi, T and Weber, B and Hile, S J and Watson, T F and van der Heijden, J and Rogge, S and Simmons, M Y},
doi = {10.1038/ncomms9848},
file = {:C$\backslash$:/praca/box/Box Sync/Tomas/structured/literature/papers/House - 2015 - Radio frequency measurements of tunnel couplings and single-triplet spin states in Si-P quantum dots.pdf:pdf},
isbn = {2041-1723 (Electronic)2041-1723 (Linking)},
issn = {2041-1723},
journal = {Nature communications},
pages = {8848},
pmid = {26548556},
publisher = {Nature Publishing Group},
title = {{Radio frequency measurements of tunnel couplings and singlet-triplet spin states in Si:P quantum dots.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26548556},
volume = {6},
year = {2015}
}
@article{Tabe2016,
abstract = {We study low-temperature transport properties of two-dimensional (2D) Si tunnel diodes, or Si Esaki diodes, with a lateral layout. In ordinary Si Esaki diodes, interband tunneling current is severely limited because of the law of momentum conservation, while nanoscale Esaki diodes may behave differently due to the dopants in the narrow depletion region, by atomistic effects which release such current limitation. In thin-Si lateral highly dopedpndiodes, we find clear signatures of interband tunneling between 2D-subbands involving phonon assistance. More importantly, the tunneling current is sharply enhanced in a narrow voltage range by resonance via a pair of a donor- and an acceptor-atom in the pn junction region. Such atomistic behavior is recognized as a general feature showing up only in nanoscale tunnel diodes. In particular, a donor-acceptor pair with deeper ground-state energies is likely to be responsible for such a sharply enhanced current peak, tunable by external biases.},
author = {Tabe, Michiharu and Tan, Hoang Nhat and Mizuno, Takeshi and Muruganathan, Manoharan and Anh, Le The and Mizuta, Hiroshi and Nuryadi, Ratno and Moraru, Daniel},
doi = {10.1063/1.4943094},
file = {:C$\backslash$:/praca/box/Box Sync/Tomas/structured/literature/papers/PN/Tabe - 2016 - Atomistic nature in band-to-band tunneling in two-dimensional silicon pn tunnel diodes.pdf:pdf},
issn = {00036951},
journal = {Applied Physics Letters},
number = {9},
title = {{Atomistic nature in band-to-band tunneling in two-dimensional silicon pn tunnel diodes}},
url = {http://dx.doi.org/10.1063/1.4943094},
volume = {108},
year = {2016}
}
